    "url": "https://github.com/rgmiranda/boom-sketch/issues"
  },
  "homepage": "https://github.com/rgmiranda/boom-sketch#readme",
  "dependencies": {
    "canvas-sketch": "^0.7.6",
    "canvas-sketch-cli": "^1.11.20",
    "canvas-sketch-util": "^1.10.0",
    "colormap": "^2.3.2",
    "eases": "^1.0.8",
    "riso-colors": "^1.0.1",
    "tone": "^14.7.77"
  }
}
